/cpp program to find factorial of a number using dynamic programming 
#include<iostream>
using namespace std;
int dp[10000];
int fact(int n)
{
  if(n<=1)
  {
    dp[n]=1;
    return 1;
  }
  if(dp[n]!=0)
   {
      return dp[n];
   }
   return dp[n]=n*fact(n-1);
}
int main()
{
  int n;
  cout<<"Enter a number:";
  cin>>n;  
  cout<<"The factorial of " <<n<<" is:" <<fact(n)<<"\n" ;
  for(int i=0;i<=n;i++)
  {
     cout<<dp[i]<<" " ;
 }
}
